<%= form_with(model: [:admin, @schedule], local: true) do |form| %>
  <%= form.hidden_field :movie_id, value: @movie.id %>
  <div>
    <%= form.label :start_time, "開始時刻" %>
    <%= form.datetime_local_field :start_time %>
  </div>
  <div>
    <%= form.label :end_time, "終了時刻" %>
    <%= form.datetime_local_field :end_time %>

  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
<%= link_to '一覧に戻る', admin_schedules_path %>
